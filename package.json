{
  "homepage": "http://yoav-zibin.github.io/NewGamePortal",
  "name": "portal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@types/prop-types": "^15.5.2",
    "konva": "^2.0.0",
    "raven-js": "^3.24.0",
    "react": "^16.3.1",
    "react-dice": "^1.0.1",
    "react-dom": "^16.3.1",
    "react-konva": "^1.7.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts-ts": "2.13.0",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "node write-sentry-version.js & react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "predeploy": "npm run build",
    "ci-test": "cross-env CI=true react-scripts-ts test --env=jsdom --forceExit",
    "deploy": "cross-env CI=true react-scripts-ts test --env=jsdom --forceExit & sentry-cli releases -o zibiga -p gameportal new `sentry-cli releases -o zibiga -p gameportal propose-version` --finalize & sentry-cli releases -o zibiga -p gameportal set-commits `sentry-cli releases -o zibiga -p gameportal propose-version` --auto & gh-pages -d build"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run ci-test && npm run build"
    }
  },
  "devDependencies": {
    "@sentry/cli": "^1.30.4",
    "@types/firebase": "^3.2.1",
    "@types/jest": "^22.2.2",
    "@types/material-ui": "^0.20.5",
    "@types/node": "^4.2.23",
    "@types/react": "^16.3.5",
    "@types/react-dom": "^16.0.3",
    "@types/react-redux": "^5.0.15",
    "@types/react-router": "^4.0.23",
    "@types/react-router-dom": "^4.2.6",
    "@types/redux-logger": "^3.0.5",
    "cross-env": "^5.1.4",
    "gh-pages": "^1.1.0",
    "husky": "^0.15.0-rc.13",
    "lint-staged": "^7.0.4",
    "material-ui": "^0.20.0",
    "prettier": "^1.11.1",
    "tslint": "^5.9.1",
    "typescript": "^2.8.1",
    "webrtc-adapter": "^6.1.4"
  }
}
